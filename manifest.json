{
  "name": "#HashtagsOnFacebook",
  "manifest_version": 2,
  "version": "2.1",
  "description": "Making hashtags feel a little more at home on Facebook.",
  "content_scripts": [
    {
      "matches": ["https://www.facebook.com/*"],
      "css": ["hashtagstyles.css"],
      "js": ["jquery-1.9.0.min.js", "hashtagscript.js"]
    }
  ],
  "options_page": "options.html",
  "permissions": [
    "storage",
    "http://*/"
  ]
}